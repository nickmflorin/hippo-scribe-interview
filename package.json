{
  "name": "hippo-sribe",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "validate-node-version": "ts-node --project=./tsconfig.json src/application/support/enforce-node-version.ts",
    "preinstall": "dotenv -e .env -e .env.local -- ts-node --project=./tsconfig.json src/application/support/enforce-node-version.ts",
    "preadd": "dotenv -e .env -e .env .env.local -- ts-node --project=./tsconfig.json src/application/support/enforce-node-version.ts",
    "analyze-build": "cross-env ANALYZE_BUNDLE=true pnpm build",
    "build": "pnpm validate-node-version && next build",
    "dev": "pnpm validate-node-version && next dev",
    "start": "pnpm validate-node-version && next start",
    "lint": "pnpm validate-node-version && next lint",
    "eslint": "pnpm validate-node-version && npx eslint .",
    "eslint-fix": "pnpm validate-node-version && npx eslint . --fix",
    "prettier-fix": "npx prettier . --write",
    "test": "pnpm validate-node-version && jest --watchAll --silent=false",
    "test:ci": "pnpm validate-node-version && jest --ci",
    "tsc": "pnpm validate-node-version && tsc --project ./tsconfig.json",
    "tsc:watch": "pnpm validate-node-version && tsc -w",
    "pullenv": "vercel env pull ./.env --environment=development",
    "pullenv-prod": "vercel env pull ./.env --environment=production"
  },
  "dependencies": {
    "@deepgram/sdk": "^3.3.2",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.4.0",
    "@next/bundle-analyzer": "^14.2.3",
    "@types/lodash.omit": "^4.5.9",
    "@types/lodash.pick": "^4.4.9",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "luxon": "^3.4.4",
    "next": "14.2.0-canary.44",
    "normalize.css": "^8.0.1",
    "openai": "^4.47.1",
    "pino": "^9.1.0",
    "pino-logflare": "^0.4.2",
    "pino-pretty": "^11.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.4",
    "react-is": "^18.3.1",
    "sass": "^1.77.2",
    "semver": "^7.5.4",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-is": "^18.3.0",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-runner-eslint": "^2.1.2",
    "jest-runner-prettier": "^1.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "utility-types": "^3.11.0"
  }
}
